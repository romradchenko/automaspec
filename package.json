{
  "name": "automaspeq",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --turbopack",
    "dev": "pnpm i && next dev",
    "start": "next start",
    "test": "vitest",
    "db:local": "turso dev --db-file db/local.db",
    "dbpl": "drizzle-kit pull --config=drizzle.config.ts",
    "dbs": "drizzle-kit studio --config=drizzle.config.ts",
    "dbg": "drizzle-kit generate --config=drizzle.config.ts",
    "dbm": "drizzle-kit migrate --config=drizzle.config.ts",
    "dbup": "pnpm dbg && pnpm dbm",
    "lint": "lefthook run pre-commit --job lint",
    "lint:all": "lefthook run pre-commit --job lint --all-files",
    "format": "lefthook run pre-commit --job format",
    "format:all": "lefthook run pre-commit --job format --all-files",
    "typecheck": "lefthook run pre-push --job typecheck",
    "precommit": "lefthook run pre-commit",
    "prepush": "lefthook run pre-push",
    "postinstall": "lefthook install"
  },
  "dependencies": {
    "@headless-tree/core": "^1.5.1",
    "@headless-tree/react": "^1.5.1",
