    "@orpc/json-schema": "^1.10.4",
    "@orpc/openapi": "^1.10.4",
    "@orpc/openapi-client": "^1.10.4",
    "@orpc/server": "^1.10.4",
    "@orpc/tanstack-query": "^1.10.4",
    "@orpc/zod": "^1.10.4",
    "@space-man/react-theme-animation": "^1.1.1",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.6",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.552.0",
    "motion": "^12.23.24",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "radix-ui": "latest",
