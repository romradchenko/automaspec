{
  "name": "automaspeq",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --webpack",
    "dev": "pnpm i && next dev",
    "deploy": "pnpm checks && vercel --prod",
    "start": "next start",
    "test": "vitest",
    "db:local": "turso dev --db-file db/local.db",
    "dbpl": "drizzle-kit pull --config=drizzle.config.ts",
    "dbs": "drizzle-kit studio --config=drizzle.config.ts",
    "dbg": "drizzle-kit generate --config=drizzle.config.ts",
    "dbm": "drizzle-kit migrate --config=drizzle.config.ts",
    "dbc": "drizzle-kit check --config=drizzle.config.ts",
    "dbup": "pnpm dbg && pnpm dbm",
    "upd": "taze --write",
    "upd-next": "next upgrade",
    "inspect-deps": "node-modules-inspector",
    "checks": "pnpm precommit",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix --fix-suggestions --fix-dangerously --deny-warnings",
    "format": "oxfmt",
    "typecheck": "next typegen && tsc --noEmit",
    "precommit": "lefthook run pre-commit",
    "prepush": "lefthook run pre-push"
  },
  "dependencies": {
    "@headless-tree/core": "^1.5.1",
    "@headless-tree/react": "^1.5.1",
    "@hookform/resolvers": "^5.2.2",
    "@libsql/client": "^0.15.15",
    "@orpc/client": "^1.12.2",
    "@orpc/contract": "^1.12.2",
    "@orpc/experimental-pino": "^1.12.2",
    "@orpc/json-schema": "^1.12.2",
    "@orpc/openapi": "^1.12.2",
    "@orpc/openapi-client": "^1.12.2",
    "@orpc/server": "^1.12.2",
    "@orpc/tanstack-query": "^1.12.2",
    "@orpc/zod": "^1.12.2",
    "@space-man/react-theme-animation": "^1.1.1",
    "@tanstack/react-form": "^1.27.0",
    "@tanstack/react-query": "^5.90.11",
    "better-auth": "^1.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.554.0",
    "motion": "^12.23.25",
    "next": "16.0.6",
    "next-themes": "^0.4.6",
    "pino": "^10.1.0",
    "radix-ui": "latest",
    "react": "19.2.0",
    "react-day-picker": "^9.11.3",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.67.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "^2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.19.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "^0.31.7",
    "jsdom": "^27.2.0",
    "lefthook": "^2.0.4",
    "node-modules-inspector": "^1.2.0",
    "oxfmt": "^0.14.0",
    "oxlint": "^1.31.0",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "taze": "^19.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vercel": "^48.12.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "lefthook",
      "sharp"
    ],
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
